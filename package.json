{
  "name": "debugtron",
  "productName": "Debugtron",
  "version": "0.5.0",
  "description": "Debug in-production Electron based App",
  "main": ".webpack/main",
  "repository": "bytedance/debugtron",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "npx prettier --check src/**/*.{ts,tsx,js,jsx}",
    "lint-fix": "npx prettier --write src/**/*.{ts,tsx,js,jsx}"
  },
  "keywords": [],
  "author": "Rongjian Zhang <pd4d10@gmail.com>",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "appBundleId": "io.github.pd4d10.debugtron",
        "icon": "assets/icon"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-zip"
        },
        {
          "name": "@electron-forge/maker-squirrel"
        },
        {
          "name": "@electron-forge/maker-dmg"
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "nodeIntegration": true,
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.js",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "bytedance",
              "name": "debugtron"
            }
          }
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "registry-js": "^1.9.0"
  },
  "devDependencies": {
    "@blueprintjs/core": "^3.15.1",
    "@electron-forge/cli": "6.0.0-beta.63",
    "@electron-forge/maker-deb": "6.0.0-beta.63",
    "@electron-forge/maker-dmg": "6.0.0-beta.63",
    "@electron-forge/maker-rpm": "6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "6.0.0-beta.63",
    "@electron-forge/maker-zip": "6.0.0-beta.63",
    "@electron-forge/plugin-webpack": "6.0.0-beta.63",
    "@electron-forge/publisher-github": "6.0.0-beta.63",
    "@types/ini": "^1.3.30",
    "@types/lodash-es": "^4.17.3",
    "@types/node-fetch": "^2.3.5",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-dropzone": "^4.2.2",
    "@types/react-redux": "^7.1.4",
    "@types/universal-analytics": "^0.4.3",
    "@types/uuid": "^3.4.4",
    "@vercel/webpack-asset-relocator-loader": "^1.7.2",
    "css-loader": "^6.7.1",
    "devtron": "^1.4.0",
    "electron": "^11.5.0",
    "electron-devtools-installer": "^3.0.0",
    "electron-redux": "^1.4.0",
    "electron-update-notification": "^0.1.0",
    "get-port": "^5.1.1",
    "ini": "^1.3.5",
    "lodash-es": "^4.17.11",
    "node-fetch": "^2.6.0",
    "node-loader": "^2.0.0",
    "node-machine-id": "^1.1.12",
    "path-browserify": "^1.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-dropzone": "^10.1.5",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "simple-plist": "^1.1.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.0",
    "typescript": "^3.7.5",
    "universal-analytics": "^0.4.20",
    "update-electron-app": "^1.5.0",
    "url-loader": "^4.1.1",
    "uuid": "^3.3.2"
  }
}
